ngx_addon_name=ngx_mod_harbour_module

if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP
    ngx_module_name=ngx_mod_harbour_module
    ngx_module_srcs="$ngx_addon_dir/ngx_mod_harbour.c"

    . auto/module
else
    HTTP_MODULES="$HTTP_MODULES ngx_mod_harbour_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_mod_harbour.c"
fi
